# Custom config for zsh-theme

PROMPT="%m@%n"
PROMPT+=' %{$FG[039]%}%c%{$reset_color%} $(git_prompt_info) %(?:%{$FG[208]%}⌦:%{$fg_bold[red]%}➜)%{$reset_color%} '

ZSH_THEME_GIT_PROMPT_PREFIX="%{$FG[241]%}on:%{$FG[036]%}"
ZSH_THEME_GIT_PROMPT_SUFFIX="%{$reset_color%}"
ZSH_THEME_GIT_PROMPT_DIRTY=" %{$FG[062]%}✘"
ZSH_THEME_GIT_PROMPT_CLEAN="%{$FG[241]%}✔"


